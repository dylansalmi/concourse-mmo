<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="MagenicMaqs" type="System.Configuration.NameValueSectionHandler" />
    <section name="RemoteSeleniumCapsMaqs" type="System.Configuration.NameValueSectionHandler" />
  </configSections>
  <MagenicMaqs>
    <!-- Root to website -->
    <add key="WebSiteBase" value="http://ttwdevopslower.azurewebsites.net/" />
    
    <!--Custom-->
    <add key="UserName" value="Ted" />
    <add key="Password" value="123" />
    
    <!--Local browser settings
    <add key="Browser" value="Chrome"/>
    <add key="Browser" value="Internet Explorer"/>
    <add key="Browser" value="Firefox"/>
    <add key="Browser" value="PhantomJS"/>
    <add key="Browser" value="Edge"/> -->
    <add key="Browser" value="Chrome" />

    <!-- Web driver hint path override - This is the first place Maqs will try to find your web drive
    <add key="WebDriverHintPath" value="C:\Frameworks"/> -->

    <!-- Remote browser settings - AKA Grid
    <add key="Browser" value="REMOTE"/> -->
    <add key="HubUrl" value="http://ondemand.saucelabs.com:80/wd/hub" />
	
<!-- On-premises Grid settings - Use RemoteSeleniumCapsMaqs for cloud based providers like Sauce Labs-->
    <!-- Remote browser
    <add key="RemoteBrowser" value="Firefox"/> -->	
      
    <!-- Extended remote (Grid) browser settings - OS (xp, win7, win8, win8.1, win10, os x, os x 10.6, os x 10.8, os x 10.9, os x 10.10, os x 10.11, solaris, linux, android, +more)
    <add key="RemotePlatform" value="win7"/>-->

    <!-- Extended remote (Grid) browser settings - Browser version
    <add key="RemoteBrowserVersion" value="44"/>-->
<!-- On-premises Grid settings -->

    <!-- Wait time in milliseconds - AKA how long do you wait for rechecking something -->
    <add key="WaitTime" value="1000" />

    <!-- Time-out in milliseconds seconds -->
    <add key="Timeout" value="10000" />

    <!-- Do you want to create logs for your tests
    <add key="Log" value="YES"/>
    <add key="Log" value="NO"/>
    <add key="Log" value="OnFail"/> -->
    <add key="Log" value="OnFail" />

    <!-- Logging Types
    <add key="LogType" value="CONSOLE"/>
    <add key="LogType" value="TXT"/> -->
    <add key="LogType" value="TXT" />
    
    <!-- What build this automation is running against -->
    <add key="Build" value="0.0.0.0" />

    <!-- Log file path - Defaults to build location if no value is defined
    <add key="FileLoggerPath" value="C:\Frameworks\"/> -->
  </MagenicMaqs>
  <RemoteSeleniumCapsMaqs>
    <!-- Cloud based Grid settings -->
    <add key="username" value="Partner_Magenic" />
    <add key="accessKey" value="Sauce_Labs_Accesskey" />
    <add key="browserName" value="Chrome" />
    <add key="platform" value="OS X 10.11" />
  </RemoteSeleniumCapsMaqs>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="nunit.framework" publicKeyToken="2638cd05610744eb" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.7.1.0" newVersion="3.7.1.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>